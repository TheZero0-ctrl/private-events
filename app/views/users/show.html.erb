<%= render "users/hosted_events"%>


<% invitation = @user.enrollments.where(:status => 0)%>
<% if !invitation.empty? %>
    <%= render "users/attended_events", head: "Invited Events", status: 0 %>
<% end %>
<% if !@user.attended_events.empty? %>
    <%= render "users/attended_events", head: "joined Events", status: 1 %>
<% end %>

<% if !@user.attended_events.empty? %>
    <%= render "users/past_events" %>
<% end %>

